    "import random as rd\n",
    "\n",
    "nb = 50\n",
    "\n",
    "fichier = open(\"coordonnees.txt\", \"w\")\n",
    "for i in range(0, nb, 2):\n",
    "        x = int(rd.randint(0, 500))\n",
    "        y = int(rd.randint(0, 500))\n",
    "        fichier.write(str(x) + \" \" + str(y) + \"\\n\")\n",
    "fichier.close()"
   ]
