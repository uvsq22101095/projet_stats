    "\n",
    "\n",
    "VARIANCE:\n",
    "    pour X:   VaX=((Xi-Xm)^2)/n\n",
    "    pour Y:   VaY=((Yi-Ym)^2)/n\n",
    "\n",
    "COVARIANCE:\n",
    "    Cov=((Xi-Xm)(Yi-Ym))/n\n",
